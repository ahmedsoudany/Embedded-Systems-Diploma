#include "stdio.h"

int main()
{
	char str[30];
	int i, count ;

	printf("Enter a string: ");
	fflush(stdout); fflush(stdin);
	gets(str);

	for (i = 0 , count = 0 ; str[i]!= '\0'; i++)
	{
		++count;
	}

	printf("The lenght of the string is: %d",count);
}
