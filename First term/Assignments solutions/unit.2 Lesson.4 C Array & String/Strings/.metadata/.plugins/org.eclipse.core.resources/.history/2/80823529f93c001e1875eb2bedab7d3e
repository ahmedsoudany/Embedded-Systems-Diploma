#include "stdio.h"
#include "string.h"

int main()
{
	char str[30] , temp[30];
	int i , j;

	printf("Enter a string: ");
	fflush(stdout);fflush(stdin);
	gets(str);

	temp[30] = str[30];

	for(i = strlen(str) , j = 0 ; i < 0 && temp != '\0'  ; i++)
	{
		str[j] = temp [i] ;
	}

	printf("Reverse string is: %s",str[30]);
}
