#include "stdio.h"

void factorial(int x);


int main()
{
	int x = 3;
	factorial(x);
	return 0;
}

void factorial(int x)
{
	int f = 1;

	if (x == 1)
		return 1;
	else
	{
		f*=x;
	    --x;
	    factorial(x);
	}
	printf("%d",f);
}
