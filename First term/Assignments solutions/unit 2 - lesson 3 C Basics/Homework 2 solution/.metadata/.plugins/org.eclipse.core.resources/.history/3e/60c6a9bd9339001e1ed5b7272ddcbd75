#include "stdio.h"

int main()
{
	char a;

	printf("Enter a character: ");
	fflush(stdout);
	scanf("%c",&a);

	if(a == (%c))
		printf("%c is a alphabet.",a);
	else
		printf("%c is not an alphabet",a);
}
